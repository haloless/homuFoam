FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//application     interFoam;
application     homuPorousInterFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         4.0;

deltaT          0.001;
//deltaT          1.0e-4;


writeControl    adjustableRunTime;

writeInterval   0.1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;
//adjustTimeStep  no;

//maxCo           1;
//maxAlphaCo      1;

maxCo           0.2;
maxAlphaCo      0.2;


maxDeltaT       1;


